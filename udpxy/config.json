{
  "name": "udpxy",
  "version": "dev",
  "slug": "udpxy",
  "description": "UDP proxy",
  "url": "https://github.com/dreyercito/addon-udpxy",
  "startup": "application",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "host_network": true,
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "map": [
    "config",
    "ssl:rw"
  ],
  "options": {
    "port": 555
  },
  "schema": {
    "port": "int"
  }
}
